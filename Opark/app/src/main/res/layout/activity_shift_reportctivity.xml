<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- <include layout="@layout/toolbar" />-->
    <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/toolBar"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/colorPrimary"
        android:elevation="4dp"
        android:theme="@style/ActionBarThemeDark"
        android:titleTextAppearance="@android:color/white"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

        <TextView
            android:id="@+id/toolbar_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="jhbcaskhcvcjh"
            android:textColor="@color/white"
            android:textSize="19sp" />
    </android.support.v7.widget.Toolbar>


    <LinearLayout
        android:id="@+id/date1"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolBar"
        android:orientation="vertical">

        <LinearLayout

            android:id="@+id/li"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/toolBar"
            android:layout_marginLeft="@dimen/_7sdp"
            android:layout_marginRight="@dimen/_7sdp"
            android:layout_marginTop="@dimen/_7sdp"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp"
            android:weightSum="2">

            <TextView
                android:id="@+id/from_date"
                android:layout_width="0dp"
                android:layout_height="@dimen/_30sdp"
                android:layout_weight="1"
                android:background="@color/color_blue"
                android:gravity="center"
                android:hint="From Date"
                android:textColor="@color/white"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

            <View
                android:id="@+id/view1"
                android:layout_width="2dp"
                android:layout_height="@dimen/_31sdp"
                android:background="@color/white" />

            <TextView
                android:id="@+id/to_date"
                android:layout_width="0dp"
                android:layout_height="@dimen/_30sdp"
                android:layout_weight="1"
                android:background="@color/color_blue"
                android:gravity="center"
                android:hint="To Date"
                android:textColor="@color/white"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_7sdp"
            android:layout_marginRight="@dimen/_7sdp"
            android:layout_marginTop="@dimen/_7sdp"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp"
            android:weightSum="2">

            <TextView
                android:id="@+id/fromTime"
                android:layout_width="0dp"
                android:layout_height="@dimen/_30sdp"
                android:layout_weight="1"
                android:background="@color/color_blue"
                android:gravity="center"
                android:hint="From Time"
                android:textColor="@color/white"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

            <View
                android:id="@+id/view2"
                android:layout_width="2dp"
                android:layout_height="@dimen/_31sdp"
                android:background="@color/white" />

            <TextView
                android:id="@+id/to_time"
                android:layout_width="0dp"
                android:layout_height="@dimen/_30sdp"
                android:layout_weight="1"
                android:background="@color/color_blue"
                android:gravity="center"
                android:hint="To Time"
                android:textColor="@color/white"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />
        </LinearLayout>

        <RadioGroup
            android:id="@+id/radioGroup"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:gravity="center_horizontal"
            android:orientation="vertical">


            <RadioButton
                android:id="@+id/twowheeler"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2 Wheeler"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/_14sdp" />


            <RadioButton
                android:id="@+id/threewheelerRB"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/d_5dp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/d_5dp"
                android:text="3 Wheeler"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/_14sdp" />

            <RadioButton
                android:id="@+id/fourwheeler"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/d_5dp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/d_5dp"
                android:text="4 Wheeler"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/_14sdp" />

            <RadioButton
                android:id="@+id/PatientVehiclee"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/d_5dp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/d_5dp"
                android:text="other"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/_14sdp" />

        </RadioGroup>


        <Button

            android:id="@+id/getReport"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/li"
            android:layout_gravity="center"
            android:background="@color/colorPrimary"
            android:padding="@dimen/_5sdp"
            android:text="Shift Report"
            android:textColor="@color/white"
            android:textStyle="bold" />
    </LinearLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/date1"
        android:layout_marginTop="@dimen/_10sdp">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <android.support.v7.widget.CardView
                android:id="@+id/card_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:visibility="gone"
                card_view:cardCornerRadius="4dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_15sdp">

                    <TextView
                        android:id="@+id/etagentName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Agent Name"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/etparkingName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etagentName"
                        android:text="Parking Name"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />


                    <TextView
                        android:id="@+id/etreportTime"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etparkingName"
                        android:text="ReportTime"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/etloginTimeshow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etreportTime"
                        android:text="loginTimeshow"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/ettimeDifferenceshow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etloginTimeshow"
                        android:text="Time Differenceshow"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />


                    <TextView
                        android:id="@+id/etparkingTypeReport"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ettimeDifferenceshow"
                        android:text="Parking Type"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/etcheckInNormal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etparkingTypeReport"
                        android:text="Normal In"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/etcheckOutNormal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etcheckInNormal"
                        android:text="Normal Check Out"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/etCheck_Outusing_Paytm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etcheckOutNormal"
                        android:text="Check Out using Paytm"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/etCheckInPass"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/etCheck_Outusing_Paytm"
                        android:text="CheckIn Pass"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />


                    <TextView
                        android:id="@+id/etCheckOutPass"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/etCheckInPass"
                        android:text="CheckOut-Pass"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/passAdd"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/etCheckOutPass"
                        android:text="Pass Add"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/passAddAmount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/passAdd"
                        android:text="Pass Add Amount"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/passRenew"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/passAddAmount"
                        android:text="Pass Renew"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/passRenewAmount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/passRenew"
                        android:text="Pass Renew Amount"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />


                    <TextView
                        android:id="@+id/etCheckInVIP"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/passRenewAmount"
                        android:text="CheckIn-VIP"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <TextView
                        android:id="@+id/etCheckOutVIP"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/etCheckInVIP"
                        android:text="CheckOut VIP"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="@dimen/_12sdp" />

                    <View
                        android:id="@+id/view"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/etCheckOutVIP"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@color/colorPrimary" />


                    <TextView
                        android:id="@+id/etCollectionCash"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/view"
                        android:layout_margin="@dimen/_3sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:text="Cash Collection"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_15sdp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/etpaytmCollection"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/etCollectionCash"
                        android:layout_margin="@dimen/_3sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:text="Paytm Collection"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_15sdp"
                        android:textStyle="bold" />

                    <View
                        android:id="@+id/viewTotal"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_below="@+id/etpaytmCollection"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@color/text_color_light_black" />


                    <TextView
                        android:id="@+id/etTotalCollection"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/viewTotal"
                        android:layout_margin="@dimen/_3sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:text="Total Collection"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_15sdp"
                        android:textStyle="bold" />


                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <RelativeLayout
                android:id="@+id/relativeLayoutReview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/card_view"
                android:background="@color/white">

                <TextView
                    android:id="@+id/park"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_3sdp"
                    android:text="  Parked Vehicle"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_15sdp"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/printlist"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_margin="@dimen/_3sdp"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_toRightOf="@+id/park"
                    android:text="     Print List   "

                    android:textColor="@color/colorred"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <View
                    android:layout_width="@dimen/_2sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_toRightOf="@+id/printlist"
                    android:background="@color/colorred"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/pdf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_margin="@dimen/_3sdp"
                    android:layout_marginRight="@dimen/_20sdp"
                    android:clickable="true"
                    android:onClick="createPdfdoc"
                    android:padding="@dimen/_1sdp"
                    android:text="Save PDF"
                    android:textColor="@color/colorred"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold"
                    android:visibility="visible" />

                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1sdp"
                    android:layout_below="@+id/pdf"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_7sdp"
                    android:layout_marginRight="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="@color/colorPrimary" />


                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerViewParkingList"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/view3">

                </android.support.v7.widget.RecyclerView>


            </RelativeLayout>

        </RelativeLayout>
    </ScrollView>

</RelativeLayout>